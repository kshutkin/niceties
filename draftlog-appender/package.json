    "name": "Konstantin Shutkin"
  },
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./core": {
      "require": "./dist/core.cjs",
      "default": "./dist/core.mjs"
